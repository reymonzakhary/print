{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "ext-fileinfo": "*",
        "ext-gettext": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-zip": "*",
        "alexusmai/laravel-file-manager": "^v3.3.0",
        "barryvdh/laravel-dompdf": "^3.0",
        "bentools/cartesian-product": "^1.4.1",
        "box/spout": "^3.3.0",
        "cmixin/business-day": "^1.19.5",
        "doctrine/dbal": "^4.2",
        "emreyarligan/enum-concern": "^1.0.5",
        "guzzlehttp/guzzle": "^7.9.3",
        "inertiajs/inertia-laravel": "^2.0",
        "intervention/image": "^3.11.2",
        "jetbrains/phpstorm-attributes": "^1.2",
        "laravel/cashier": "^15.0",
        "laravel/framework": "^12.0",
        "laravel/passport": "^12.0",
        "laravel/tinker": "^2.10",
        "laravel/ui": "^4.0",
        "lcobucci/jwt": "^5.0",
        "league/flysystem-aws-s3-v3": "^3.29.0",
        "marvinlabs/laravel-discord-logger": "^v1.4.3",
        "moneyphp/money": "^4.0",
        "mtownsend/xml-to-array": "^2.0.0",
        "nwidart/laravel-modules": "^12",
        "pda/pheanstalk": "^5.0",
        "predis/predis": "^3.2",
        "pusher/pusher-php-server": "^7.2.7",
        "ramsey/uuid": "^4.7.6",
        "react/promise": "^v2.11.0",
        "santigarcor/laratrust": "^8.4.1",
        "setasign/fpdi": "^v2.6.3",
        "smalot/pdfparser": "^v2.12.0",
        "spatie/eloquent-sortable": "^4.4.2",
        "spatie/laravel-tags": "^4.10.0",
        "spatie/pdf-to-text": "^1.54.0",
        "spatie/simple-excel": "^3.7.2",
        "stancl/tenancy": "^3.9",
        "staudenmeir/laravel-adjacency-list": "^v1.20.1",
        "webklex/laravel-pdfmerger": "^1.3.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15",
        "fakerphp/faker": "^1.24",
        "laravel/telescope": "^5.0",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0",
        "spatie/laravel-ignition": "^2.9"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        },"merge-plugin": {
            "include": [
                "Modules/*/composer.json"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Database\\Seeders\\Tenants\\": "database/seeders/tenants/"
        },
        "classmap": [
            "database/seeders",
            "database/seeders/tenants",
            "database/factories"
        ],
        "files": [
            "app/Helpers/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php -d memory_limit=-1"
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }

}
